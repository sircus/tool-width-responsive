{
  "name": "sircus-tools-width-responsive",
  "description": "Responsive css width tools Module for Sircus.",
  "version": "0.0.2",
  "homepage": "http://sircus.github.io",
  "keywords": [
    "scalabe",
    "design",
    "css",
    "cssnext",
    "rwd"
  ],
  "author": {
    "name": "Yasuyuki Enomoto",
    "url": "http://blivesta.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircus/tools-width-responsive.git"
  },
  "scripts": {
    "build:sm": "css2scss ./lib/sm-width.css ./scss/_sm-width.scss",
    "build:md": "css2scss ./lib/md-width.css ./scss/_md-width.scss",
    "build:lg": "css2scss ./lib/lg-width.css ./scss/_lg-width.scss",
    "build": "npm run build:sm && npm run build:md && npm run build:lg"
  },
  "dependencies": {
    "sircus-global-property": "^0.0.1"
  },
  "devDependencies": {
    "sircus-process-css2scss": "^0.2.0"
  }
}
